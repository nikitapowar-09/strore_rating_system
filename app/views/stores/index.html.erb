<h1>Stores</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Address</th>
      <th>Rating</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @stores.each do |store| %>
      <tr>
        <td><%= store.name %></td>
        <td><%= store.email %></td>
        <td><%= store.address %></td>
        <td><%= store.rating %></td>
        <td>
          <%= link_to 'Edit', edit_admin_store_path(store) %>
          <%= link_to 'Destroy', admin_store_path(store), method: :delete, data: { confirm: 'Are you sure?' } %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
